<p id="notice"><%= notice %></p>

<h1>Resultados</h1>

<table>
  <thead>
    <tr>
      <th>Resultado</th>
      <th>Puntaje max</th>
      <th>Punteje obtenido</th>
      <th>Integer</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @resultados.each do |resultado| %>
      <tr>
        <td><%= resultado.resultado_id %></td>
        <td><%= resultado.puntaje_max %></td>
        <td><%= resultado.punteje_obtenido %></td>
        <td><%= resultado.integer %></td>
        <td><%= link_to 'Show', resultado %></td>
        <td><%= link_to 'Edit', edit_resultado_path(resultado) %></td>
        <td><%= link_to 'Destroy', resultado, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Resultado', new_resultado_path %>
